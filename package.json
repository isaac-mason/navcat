{
    "name": "navcat",
    "version": "0.2.0",
    "author": "Isaac Mason <isaac@isaacmason.com>",
    "description": "",
    "keywords": [],
    "license": "MIT",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/src/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/src/index.d.ts"
        },
        "./blocks": {
            "import": "./dist/blocks.js",
            "types": "./dist/blocks/index.d.ts"
        },
        "./three": {
            "import": "./dist/three.js",
            "types": "./dist/three/index.d.ts"
        }
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/isaac-mason/navcat.git"
    },
    "scripts": {
        "build": "rm -rf ./dist && rollup --config rollup.config.mjs",
        "format": "biome format --write src/.",
        "lint": "biome lint --write src/.",
        "docs": "cd docs && node ./build.js",
        "typedoc": "typedoc --tsconfig ./tsconfig.json --name \"navcat docs\" --out ./dist-typedoc ./src/index.ts ./blocks/index.ts ./three/index.ts",
        "test": "vitest"
    },
    "packageManager": "pnpm@8.15.2+sha1.798034f615aa211c6c2ae00010cc20be5fc102ee",
    "dependencies": {
        "mathcat": "0.0.11"
    },
    "peerDependencies": {
        "three": ">=0.180.0"
    },
    "peerDependenciesMeta": {
        "three": { 
            "optional": true
        }
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.0",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-typescript": "^12.1.4",
        "@types/three": "0.183.0",
        "rollup": "^4.46.2",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-filesize": "^10.0.0",
        "tslib": "^2.8.1",
        "typedoc": "^0.28.14",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    }
}
